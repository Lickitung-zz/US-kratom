extends ../partials/layout

//- Override default config variables
append config
  - var pageTitle = 'Pagination'
  - var isNestedFile = true
  - var parentMenu = 'Components'
  - var breadcrumbs = [{'title': 'Home', 'link': '../index.html'}, {'title': 'Components', 'link': 'accordion.html'}, {'title': 'Pagination'}]

//- Page Content
block content
  .container.padding-bottom-3x.mb-2
    .row
      .col-lg-9.col-md-8.order-md-2
        h6.text-muted.text-lg.text-uppercase.margin-bottom-1x Pagination
        include ../partials/pagination
        h6.text-muted.text-lg.text-uppercase.padding-top-2x.mt-2.margin-bottom-1x Entry Navigation
        include ../partials/entry-navigation
      
      .col-lg-3.col-md-4.order-md-1
        //- Side Menu
        include ../partials/side-menu