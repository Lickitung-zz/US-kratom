// Pagination
nav.pagination
  .column
    ul.pages
      li.active: a(href='#') 1
      li: a(href='#') 2
      li: a(href='#') 3
      li: a(href='#') 4
      li ...
      li: a(href='#') 12
  .column.text-right.hidden-xs-down
    a(href='#').btn.btn-outline-secondary.btn-sm
      | Next&nbsp;
      i.icon-arrow-right