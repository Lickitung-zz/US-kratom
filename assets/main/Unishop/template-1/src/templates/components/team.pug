              .card-body
                h5.card-title.mb-2 Clare Barrera
                p.text-muted.mb-2 Art Director
                .social-bar
                  a(href='#' data-toggle='tooltip' data-placement='top' title='Behance').social-button.shape-circle.sb-behance
                    i.socicon-behance
                  a(href='#' data-toggle='tooltip' data-placement='top' title='Dribbble').social-button.shape-circle.sb-dribbble
                    i.socicon-dribbble
                  a(href='#' data-toggle='tooltip' data-placement='top' title='Instagram').social-button.shape-circle.sb-instagram
                    i.socicon-instagram
          .col-sm-4.mb-4
            .card.text-center
              img(src='../img/team/08.jpg' alt='Team').card-img-top
              .card-body
                h5.card-title.mb-2 Joshua Andrews
                p.text-muted.mb-2 Software Engeneer
                .social-bar
                  a(href='#' data-toggle='tooltip' data-placement='top' title='GitHub').social-button.shape-circle.sb-github
                    i.socicon-github
                  a(href='#' data-toggle='tooltip' data-placement='top' title='Twitter').social-button.shape-circle.sb-twitter
                    i.socicon-twitter
                  a(href='#' data-toggle='tooltip' data-placement='top' title='Stack Overflow').social-button.shape-circle.sb-stackoverflow
                    i.socicon-stackoverflow
      
      .col-lg-3.col-md-4.order-md-1
        //- Side Menu
        include ../partials/side-menu